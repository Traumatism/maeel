{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "maeel",
	"patterns": [
		{
			"name": "constant.language.boolean",
			"match": "(α|β)"
		},
		{
			"name": "keyword.control.my-language",
			"match": "(⟹|⇒|ω|Ω)"
		},
		{
			"name": "constant.numeric.my-language",
			"match": "\\b\\d+(\\.\\d+)?\\b"
		},
		{
			"name": "punctuation.definition.block.start.my-language",
			"match": "\\("
		},
		{
			"name": "punctuation.definition.block.end.my-language",
			"match": "\\)"
		},
		{
			"name": "string.quoted.double.my-language",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.my-language",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "variable.other.constant.my-language",
			"match": "\\b([a-zA-Z_]+[a-zA-Z0-9_]*)\\b"
		},
		{
			"name": "keyword.operator.my-language",
			"match": "(¬|!|-|∪|∨|\\+|∧|\\*|⊕|⊻|/|%|∣|∤|ρ|δ|ψ|θ|σ|ζ|≕|→|⟶|λ|∅|ε|π|<|≤|⩽|>|≥|⩾|=|≠|±|∓)"
		}
	],
	"scopeName": "source.maeel"
}