{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "maeel",
	"patterns": [
		{
			"include": "#strings"
		},
		{
			"include": "#booleans"
		},
		{
			"include": "#numerics"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#names"
		},
		{
			"include": "#operators"
		}
	],
	"repository": {
		"strings": {
			"name": "string.quoted.double.maeel",
			"begin": "\"",
			"end": "\""
		},
		"booleans": {
			"patterns": [
				{
					"name": "constant.language.maeel",
					"match": "\\b(α|β)\\b"
				}
			]
		},
		"numerics": {
			"patterns": [
				{
					"name": "constant.numeric.maeel",
					"match": "\\b([0-9\\._]+)\\b"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.maeel",
					"match": "\\b(⟹|ω|Ω|@|λ|include|↑)\\b"
				},
				{
					"name": "entity.name.function.maeel",
					"match": "\\b(ρ|δ|Σ|Π|θ|σ|τ|Γ|clear|ψ|print)\\b"
				}
			]
		},
		"names": {
			"patterns": [
				{
					"name": "support.variable.maeel",
					"match": "\\b([a-zA-Z0-9_]+)\\b"
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator",
					"match": "\\b(\\+|\\-|\\*|\\/|%|<|>|=|!)\\b"
				}
			]
		}
	},
	"scopeName": "source.maeel"
}