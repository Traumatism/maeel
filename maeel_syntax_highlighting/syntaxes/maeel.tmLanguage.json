{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "maeel",
	"scopeName": "source.maeel",
	"fileTypes": [
		"maeel"
	],
	"patterns": [
		{
			"begin": "(",
			"beginCaptures": {
				"0": {
					"name": "puncuation.section.block.begin.maeel"
				}
			},
			"end": ")",
			"endCaptures": {
				"0": {
					"name": "puncuation.section.block.end.maeel"
				}
			},
			"name": "meta.block.maeel",
			"patterns": [
				{
					"include": "#code"
				}
			]
		},
		{
			"name": "keyword.control.maeel",
			"match": "(Ω|⟹|⇒|ω|λ)"
		},
		{
			"name": "keyword.operator.lodical.maeel",
			"match": "(¬|∧|∨|⊕|⊻)"
		},
		{
			"name": "keyword.operator.arithmetic.maeel",
			"match": "(\\-|\\+|\\*|\/|%|∣|∤|!)"
		},
		{
			"name": "keyword.operator.comparison.maeel",
			"match": "(=|≠|≥|⩾|≤|⩽|<|>)"
		},
		{
			"name": "support.function.maeel",
			"match": "(ρ|δ|ψ|θ|σ|ζ|τ|Γ|Π|Σ|#|±|∓|↓|↑|\\bprint\\b)"
		},
		{
			"name": "constant.language.boolean.maeel",
			"match": "(α|β)"
		},
		{
			"name": "constant.numeric.maeel",
			"match": "([0-9]+(\\.?[0-9]+)?)"
		},
		{
			"name": "entity.name",
			"match": "([a-zA-Z_][a-zA-Z0-9_]*)"
		},
		{
			"name": "string.quoted.double.maeel",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.maeel",
					"match": "\\\\(?:[0\"\\abfnrtv]|x[0-9A-Fa-f]{2}|u[0-9A-Fa-f]{4}|U[0-9A-Fa-f]{4})"
				}
			]
		}
	]
}