proc _pow do
    let exp  pop
    let base pop

    1 0

    dup exp < while do
        swap base *
        swap 1 +
        dup exp <
    end

    pop
end

proc pow do
    let exp  dup swap
    let base pop

    0 < dup

    if do
        base exp! _pow 1 swap / swap
    end

    !if do
        base exp _pow
    end
end

2 10 pow println
2 10! pow println