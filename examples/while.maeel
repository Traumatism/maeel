@ recursive while loop implementation
@
@ warning: may cause stack overflow

proc while_inner
    a b= if
        return
    end a b =! if
        "still in loop, a != b" println pop
        a 1+ let a pop
        while_inner
    end
end_proc

proc while_maeel
    let a pop
    let b pop
    while_inner
end_proc

10 0 while_maeel
