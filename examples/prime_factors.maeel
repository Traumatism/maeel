"std" include

fun prime_factors n
(
    2 -> i
    {} -> factors

    i i * n lteq while (
        i -> i_tmp

        n i_tmp divides not => (i inc -> i)

        n i_tmp divides => (
            n n i_tmp % - i_tmp / -> n
            factors i_tmp + -> factors
        )

        i i * n lteq
    )

    n 1 > => (factors n + -> factors)

    factors
)

2560 prime_factors puts
